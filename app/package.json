{
  "name": "acc-ui",
  "version": "1.0.0",
  "description": "External frontend for innovate uk acc system",
  "scripts": {
    "script:check-node": "node ./scripts/validate-node-version.js",
    "clean": "rimraf ./dist ./public/build",
    "esbuild": "node --enable-source-maps esbuild.js --target acc",
    "esbuild:tsc": "node --enable-source-maps esbuild.js --tsc --target acc",
    "start": "node -r dotenv/config esbuild.js --watch --tsc --devtools --target acc",
    "start:server": "node --enable-source-maps ./dist/src/server/index.js",
    "serve": "node --inspect --enable-source-maps -r dotenv/config ./dist/src/server/index.js",
    "postinstall": "npm run patch-package",
    "patch-package": "patch-package",
    "build": "webpack --env env=production",
    "build:dev": "webpack --env env=development --env devtools",
    "test": "TZ=europe/london jest",
    "test:light": "TS_JEST_ISOLATED_MODULES=true jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:staged": "lint-staged",
    "lint:fix": "npm run lint -- --fix",
    "nuke": "npm run clean && rimraf node_modules",
    "update-govfrontend": "node ./scripts/update-govfrontend",
    "update": "npm-check -u",
    "cy": "cd ../cypress-test && npm start",
    "relay": "relay-compiler",
    "typecheck": "tsc --watch --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "prettier": "prettier --check --ignore-path ../.prettierignore .",
    "prettier:write": "prettier --write --ignore-path ../.prettierignore .",
    "updateSchema": "node --inspect -r dotenv/config ./scripts/updateSchema/dist/index.js",
    "updateSchema:build": "node --enable-source-maps esbuild.js --tsc --target updateSchema"
  },
  "author": "BJSS Ltd",
