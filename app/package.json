{
  "name": "acc-ui",
  "version": "1.0.0",
  "description": "External frontend for innovate uk acc system",
  "scripts": {
    "script:check-node": "node ./scripts/validate-node-version.js",
    "clean": "rimraf ./dist ./public/build",
    "start:server": "node ./dist/src/server/index.js",
    "start:dev": "nodemon ./dist/src/server/index.js --watch ./dist/src/ --watch ./.env --watch ./public/build/",
    "build:server": "npm run clean && tsc",
    "watch:server": "npm run clean && tsc --watch",
    "build:client": "webpack --config webpack.client.prod.js",
    "build:client:dev": "webpack --config webpack.client.dev.js",
    "watch:client": "npm run build:client:dev -- --watch",
    "watch": "npm-run-all --parallel script:check-node watch:client watch:server",
    "watch:all": "npm-run-all --parallel watch:client watch:server start:dev",
    "build": "npm run build:server && npm run build:client",
    "build:dev": "npm run build:server && npm run build:client:dev",
    "test": "jest",
    "test:watch": "jest --coverage --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "nuke": "rimraf node_modules && npm run clean",
    "update": "npm-check -u"
  },
  "author": "BJSS Ltd",
  "license": "ISC",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Samsung versions",
    "last 2 Edge versions",
    "ie 11",
    "last 2 Safari versions",
    "iOS >= 12"
  ],
