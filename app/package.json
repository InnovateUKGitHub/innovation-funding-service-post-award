{
  "name": "acc-ui",
  "version": "1.0.0",
  "description": "External frontend for innovate uk acc system",
  "scripts": {
    "script:check-node": "node ./scripts/validate-node-version.js",
    "clean": "rimraf ./dist ./public/build",
    "esbuild": "node --enable-source-maps esbuild.js",
    "esbuild:tsc": "node --enable-source-maps esbuild.js --tsc",
    "start": "node --enable-source-maps esbuild.js --watch --tsc",
    "start:server": "node --enable-source-maps ./dist/src/server/index.js",
    "serve": "node -r dotenv/config ./dist/src/server/index.js",
    "postinstall": "npm run patch-package",
    "patch-package": "patch-package",
    "prebuild:server": "npm run clean",
    "build:server": "tsc",
    "watch:server": "npm run clean && tsc --watch",
    "build:client": "webpack --config webpack.client.prod.js",
    "build:client:dev": "webpack --config webpack.client.dev.js",
    "watch:client": "npm run build:client:dev -- --watch",
    "watch": "npm-run-all --parallel script:check-node watch:client watch:server",
    "watch:all": "npm-run-all --parallel watch:client watch:server start:dev",
    "build": "npm run build:server && npm run build:client",
    "build:dev": "npm run build:server && npm run build:client:dev",
    "test": "jest",
    "test:light": "TS_JEST_ISOLATED_MODULES=true jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "nuke": "npm run clean && rimraf node_modules",
    "update-govfrontend": "node ./scripts/update-govfrontend",
    "update": "npm-check -u",
    "prepare": "cd .. && husky install"
  },
  "author": "BJSS Ltd",
  "license": "ISC",
  "browserslist": [
    "last 6 Chrome versions",
    "last 6 Firefox versions",
    "last 6 Samsung versions",
    "last 6 Edge versions",
    "ie 11",
    "last 6 Safari versions",
    "iOS >= 12"
  ],
  "dependencies": {
