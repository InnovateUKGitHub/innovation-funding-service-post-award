{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom", "es2019"],
    "types": ["cypress", "node", "@testing-library/cypress"],
    "baseUrl": "./cypress",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node"
  },
  "include": ["**/*.ts", "launch.js"]
}
